{
  "name": "apollo-time-travel-debugger",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Apollo time-travel debugger",
  "offline_enabled": false,
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "permissions": ["webRequest", "*://*/*"],
  "devtools_page": "devtools.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
